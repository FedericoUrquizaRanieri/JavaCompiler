.DATA
lblVTInit: NOP

.CODE
lblConstructor@Init:LOADFP
LOADSP
STOREFP
FMEM 0
STOREFP
RET 1

lblMetmain@Init: LOADFP
LOADSP
STOREFP
RMEM 1
RMEM 1
PUSH 2 ; atributos y VT
PUSH simple_malloc ; reservar heap
CALL
DUP
PUSH lblVTStorageTest ; Etiqueta de la VT
STOREREF 0
DUP
PUSH lblConstructor@StorageTest
CALL
STORE 0
LOAD 0 ; Cargo var
PUSH 77
SWAP ; Muevo this
DUP ; Duplico this
LOADREF 0 ; Cargo VT
LOADREF 0 ; Cargo metodo setValue
CALL
LOAD 0 ; Cargo var
RMEM 1 ; Reservo retorno
SWAP
DUP ; Duplico this
LOADREF 0 ; Cargo VT
LOADREF 1 ; Cargo metodo getValue
CALL
PUSH lblMetdebugPrint@Object
CALL
FMEM 1
STOREFP
RET 0
.DATA
lblVTStorageTest: DW lblMetsetValue@StorageTest,lblMetgetValue@StorageTest

.CODE
lblConstructor@StorageTest:LOADFP
LOADSP
STOREFP
FMEM 0
STOREFP
RET 1

lblMetsetValue@StorageTest: LOADFP
LOADSP
STOREFP
LOAD 4 ; Cargo var
LOAD 3 ; Acceso atributo
SWAP
STOREREF 1 ; Guardo en atributo
FMEM 0
STOREFP
RET 2

lblMetgetValue@StorageTest: LOADFP
LOADSP
STOREFP
LOAD 3 ; Acceso atributo
LOADREF 1 ; Cargo atributo
STORE 4; Guardo retorno
STOREFP ; Usa ED para volver a RA llamador
RET 1 ; Libera los parametros y retorna de la unidad
FMEM 0
STOREFP
RET 1
