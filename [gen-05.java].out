.DATA
lblVTInit: NOP

.CODE
lblConstructor@Init:LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
STOREFP ; Almacena el tope de la pila en el registro fp
RET 1 ; Libera los parametros y retorna de la unidad

lblMetmain@Init: LOADFP
LOADSP
STOREFP
RMEM 1
RMEM 1
PUSH 2 ; atributos y VT
PUSH simple_malloc ; reservar heap
CALL
DUP
PUSH lblVTStorageTest ; Etiqueta de la VT
STOREREF 0
DUP
PUSH lblConstructor@StorageTest
CALL
STORE 0
LOAD 0 ; Cargo la direccion de var
PUSH 77
SWAP ; Muevo this
DUP ; Duplico this
LOADREF 0 ; Cargo VT
LOADREF 0 ; Cargo metodo setValue
CALL ; Llamo metodo
LOAD 0 ; Cargo la direccion de var
RMEM 1 ; Reservo lugar para el retorno
SWAP
DUP ; Duplico this
LOADREF 0 ; Cargo VT
LOADREF 1 ; Cargo metodo getValue
CALL ; Llamo metodo
PUSH lblMetdebugPrint@Object
CALL
FMEM 1
STOREFP
RET 0 ; Libera los parametros y retorna de la unidad
.DATA
lblVTStorageTest: DW lblMetsetValue@StorageTest,lblMetgetValue@StorageTest

.CODE
lblConstructor@StorageTest:LOADFP ; Apila el valor del registro fp
LOADSP ; Apila el valor del registro sp
STOREFP ; Almacena el tope de la pila en el registro fp
FMEM 0
STOREFP ; Almacena el tope de la pila en el registro fp
RET 1 ; Libera los parametros y retorna de la unidad

lblMetsetValue@StorageTest: LOADFP
LOADSP
STOREFP
LOAD 4 ; Cargo la direccion de var
LOAD 3 ; Acceso a atributo
SWAP ; Muevo this a SP - 1
STOREREF 1 ; Guardo valor en atributo
FMEM 0
STOREFP
RET 2 ; Libera los parametros y retorna de la unidad

lblMetgetValue@StorageTest: LOADFP
LOADSP
STOREFP
LOAD 3 ; Acceso a atributo
LOADREF 1 ; Cargo direccion de atributo
STORE 4; Guardo retorno
STOREFP ; Usa ED para volver a RA llamador
RET 1 ; Libera los parametros y retorna de la unidad
FMEM 0
STOREFP
RET 1 ; Libera los parametros y retorna de la unidad
